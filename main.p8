--bullets

function shoot()
	new_bullet={
	x=px,
	y=py,
	speedx=4,
	speedy=4
	}
	
	--tir bas
	if player_direction==ps1
	then
		new_bullet.speedy=-1
		new_bullet.speedx=0
		end
			--tir haut
	if player_direction==ps3
	then
		new_bullet.speedy=1
		new_bullet.speedx=0
		end
			--tir droite
	if player_direction==ps2
	then
		new_bullet.speedx=-1
		new_bullet.speedy=0
		end
			--tir gauche
	if player_direction==ps4
	then
		new_bullet.speedx=1
		new_bullet.speedy=0
		end
		
		add(bullets,new_bullet)
		sfx(0)
end
		
	function update_bullets()
		for b in all(bullets) do
			b.x-=b.speedx
			b.y-=b.speedy
	end
end
