--personnages

function make_finger()
	px=10
	py=10
	ps1=1 --bas
	ps2=17 --droite
	ps3=33 --haut
	ps4=49 --gauche
	player_direction=ps3
	
end



function draw_finger()
  if btn(⬅️) then
  	spr(ps4,px,py)
  	player_direction=ps4
  elseif btn(➡️) then
  	spr(ps2,px,py)
  	player_direction=ps2
 	elseif btn(⬆️) then
  	spr(ps3,px,py)
  	player_direction=ps3
  elseif btn(⬇️) then
   spr(ps1,px,py)
   player_direction=ps1
  else
  	spr(player_direction,px,py)
 end
  
end

