__lua__

function _init()
	make_finger()
	bullets={}
	ennemis={}
	music(0)
end

function _update()
	move_finger()
	check_flag()
	if (btnp(❎)) shoot()
	update_bullets()
	update_ennemis()
	if #ennemis==0 then
	spawn_ennemis(100)
	--sfx(1,01,01) --game over
	end
end
function _draw()
	cls()
	map(0,0,0,0,128,64)
	draw_finger()
	cam()
	--print(check_flag(0,px/8,py/8),5)
	for e in all(ennemis) do
		spr(10,e.x,e.y)
	end
	for b in all(bullets) do
		spr(29,b.x,b.y)
	end
	draw_ennemis()
	onpop_hand()
end

