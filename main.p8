--deplacement personnage

function move_finger()
	
	if (btn(⬅️)) 
	then
		local targetx=px-8
			if check_flag(0,ceil(targetx/8),py/8) == false
			then
			px-=2
		end
	end
	if (btn(➡️)) 
	then
		local targetx=px+2
			if check_flag(0,ceil(targetx/8),py/8) == false
	  		then
	  		px+=2
	 	end
	end 
	if (btn(⬆️))
	then
	 	local targety=py-2
	 		if	check_flag(0,ceil(px/8),targety/8) == false
	  		then
	  		py-=2
	 	end
	end 
	if (btn(⬇️))
	then
		local targety=py+8
			if check_flag(0,ceil(px/8),targety/8) == false
	  		then
	  		py+=2
	 	end
	end  
	

end
	
