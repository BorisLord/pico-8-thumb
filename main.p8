function onpop_hand()
 casehand= {x=56*8,y=4*8}
 finger={x=px,y=py}
 
	if collision(casehand,finger)
	then
	spr(30,casehand.x, casehand.y) 
	spr(40,casehand.x, 
	casehand.y,2,2)
	end
end
