--ennemie

function spawn_ennemis(nombre)
 for i=1,nombre do
 add(ennemis,
 {x=ceil(rnd(550)),
  y=ceil(rnd(250)),
  direction=rnd({-1,1})
 })
 end
end

function update_ennemis()
	for e in all(ennemis) do
		if e.direction==-1 then
			e.y+=0.5
		else
			e.y-=0.5
		end
		if e.direction==-1 and
			check_flag(0,ceil(e.x/8),
			ceil(e.y/8)) then
			e.direction=1
		end
		if e.direction==1 and
			check_flag(0,ceil(e.x/8),
			ceil(e.y/8)-1) then
			e.direction=-1
		end
end

function draw_ennemis()
	for e in all(ennemis) do
	spr(10,e.x,e.y)
 end
end

	--collision pour chaque balle
	for b in all(bullets) do
		for e in all(ennemis) do
			if collision(e,b) then			
				del(bullets,b)
				e.life=-1
				del(ennemis,e)
				--if e.life==0 then
					--del(enemies,e)
				--end
			end
		end
	end
	
	
end
