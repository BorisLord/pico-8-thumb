--map + camera moving

function cam()
	camx=px
	camy=py
	casex=flr(px/8)
	casey=flr(py/8)
	--blocage bdm
	if (px<0) px=0
	if (py<0) py=0
	if (px>(64*8)-1) px=511
	if (py>(32*8)-1) py=255
	
	local camx=flr(casex/16)*16
	local camy=flr(casey/16)*16
	camera(camx*8,camy*8)
end
